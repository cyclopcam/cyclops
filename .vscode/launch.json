		{
			"name": "cyclops.go",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "cmd/cyclops/cyclops.go",
			"cwd": "${workspaceFolder}",
			"args": [
				"--hot"
			]
		},
		{
			"name": "proxy.go",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "cmd/proxy/proxy.go"
