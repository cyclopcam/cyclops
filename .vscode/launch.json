		{
			"name": "kernelwg.go",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "cmd/kernelwg/kernelwg.go",
			"asRoot": false,
			"console": "integratedTerminal"
		},
		{
			"name": "Current Go File",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${file}"
		}
	]
